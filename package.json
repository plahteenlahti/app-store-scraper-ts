{
  "name": "app-store-scraper-ts",
  "version": "1.0.1",
  "description": "Modern TypeScript library to scrape application data from the iTunes/Mac App Store",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:run": "vitest run",
    "example": "tsx examples/all-methods.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run typecheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plahteenlahti/app-store-scraper-ts.git"
  },
  "bugs": {
    "url": "https://github.com/plahteenlahti/app-store-scraper-ts/issues"
  },
  "homepage": "https://github.com/plahteenlahti/app-store-scraper-ts#readme",
  "keywords": [
    "app-store",
    "itunes",
    "scraper",
    "ios",
    "ipad",
    "mac",
    "apple",
    "typescript",
    "app-data",
    "reviews",
    "ratings"
  ],
  "author": "Perttu LÃ¤hteenlahti",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "fast-xml-parser": "^4.3.2",
    "p-memoize": "^7.1.1",
    "p-throttle": "^6.1.0",
    "undici": "^6.0.0",
    "zod": "^4.1.12"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
